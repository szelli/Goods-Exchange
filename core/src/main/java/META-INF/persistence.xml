<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="core" transaction-type="RESOURCE_LOCAL">
		<class>com.szpzs.model.City</class>
		<class>com.szpzs.model.ForRent</class>
		<class>com.szpzs.model.Message</class>
		<class>com.szpzs.model.Rate</class>
		<class>com.szpzs.model.Reserved</class>
		<class>com.szpzs.model.User</class>
		<class>com.szpzs.model.Category</class>
		<class>com.szpzs.model.Picture</class>
		<class>com.szpzs.model.Product</class>
		<class>com.szpzs.model.Role</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@obiwan:1521:orcl2"/>
			<property name="javax.persistence.jdbc.user" value="goods_exchange"/>
			<property name="javax.persistence.jdbc.password" value="goods_exchange"/>
			<property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
		</properties>
	</persistence-unit>
</persistence>
